<div>
    <h1>Creation of a new Review </h1>
    <%= simple_form_for  [@parking, @review] do |f| %>
        <%= f.input :content, html5: true, required: true %>
        <%= f.input :rating, collection: 1..5 %>
        <%= f.submit 'Create Review' %>
    <% end %>
</div>

<%= link_to  "Back", parking_reviews_path(@parking) %>

