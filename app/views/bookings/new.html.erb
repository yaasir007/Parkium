<div class="container", style="width:40%">
  <h3 align="center">Booking Form</h3>
  <%= simple_form_for [@parking, @booking], method: :post do |f| %>
            <%= f.input :date %>
            <%= f.input :start_time %>
            <%= f.input :end_time %>
            <div align="center">
              <%= f.button :submit, "Book Me" , class: "btn btn-success" %>
  <% end %>
  <%= link_to  "Back", parking_path(@parking), class: "btn btn-dark" %>
</div>
