<div align="center" style="padding-top:160px;">
    <h1>New For Admins</h1>

    <%= simple_form_for [@parking] do |f| %>
    <%= f.input :name, html5: true %>
    <%= f.input :address, html5: true %>
    <%= f.input :description, html5: true %>
    <%= f.input :number_of_slots, html5: true %>
    <%= f.input :fees %>
    
     <%= f.submit 'Create Review' %>
    <% end %>
</div>